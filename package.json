    "@reach/visually-hidden": "^0.10.4",
    "cross-env": "^7.0.3",
    "gatsby": "^2.29.2",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-feed-mdx": "^1.0.1",
