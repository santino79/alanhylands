    "gatsby-remark-images": "^3.3.23",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-remark-widows": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-source-graphql": "^2.11.0",
